{
  "reverseProxies": [
    {
      "route": "/api2",
      "remoteServer": "http://localhost:3000"
    },
    {
      "route": "/api",
      "remoteServer": "http://localhost:3001"
    }
  ],
  "staticApps": [
    {
      "route": "/app",
      "directory": "./public"
    },
    {
      "route": "/static",
      "directory": "./static"
    }
  ],
  "port": 4200,
  "metrics": {
    "endpoint": "/actuator/prometheus"
  },
  "logLevel": "INFO"
}
